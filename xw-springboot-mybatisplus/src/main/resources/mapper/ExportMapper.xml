<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xx.xw.stmp.mapper.IExportMapper">
    <!--流式查询 pgsql 要开启事务，并且设置为手动提交才能开启流式查询 且resultSetType="FORWARD_ONLY"-->
    <!--
        流式查询 mysql fetchSize 要设置为Integer的最小值，且resultSetType="FORWARD_ONLY" 才能开启流式查询，
        另外，如果url中设置useCursorFetch=true时，将开启游标查询，此时fetchSize可以不设置为Integer的最小值
    -->
    <select id="getAll" resultType="com.xx.xw.stmp.pojo.entity.ExportEntity" fetchSize="10" resultSetType="FORWARD_ONLY">
        select * from t_export
    </select>

   <!-- <select id="getAll" resultType="com.xx.xw.stmp.pojo.entity.ExportEntity" fetchSize="-2147483648" resultSetType="FORWARD_ONLY">
        select * from t_export
    </select>-->

</mapper>